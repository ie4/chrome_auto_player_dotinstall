{
  "name": "ドットインストール連続再生",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "ドットインストールの動画を連続再生します",

  "permissions": [
    "tabs"
  ],

  "content_scripts": [
    {
      "matches": ["http://dotinstall.com/lessons/*"],
      "js": ["js/jquery-2.1.4.min.js","js/content.js"]
    }
  ],
  "background": {
    "scripts": ["js/background.js"]
  }

}